AddKeysToAgent yes

IdentitiesOnly yes

Host tsomkes
    HostName tsomkes.com
    User tsomkes
    IdentityFile ~/.ssh/id_ts_rsa
    ServerAliveInterval 30

Host bitbucket.org-TSomKes
    User git
    IdentityFile ~/.ssh/id_ts_rsa

    # Making this work at the library
    HostName altssh.bitbucket.org
    Port 443

Host bitbucket.org-cekor1
    User git
    IdentityFile ~/.ssh/cekor1_id_rsa

    # Making this work at the library
    HostName altssh.bitbucket.org
    Port 443

Host bitbucket.org-GogyUp
	User git
    IdentityFile ~/.ssh/gogy_id_rsa

    # Making this work at the library
    HostName altssh.bitbucket.org
    Port 443

Host github.com
	User git
    IdentityFile ~/.ssh/id_ts_rsa

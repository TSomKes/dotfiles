AddKeysToAgent yes

IdentitiesOnly yes

Host tsomkes
    HostName tsomkes.com
    User tsomkes
    IdentityFile ~/.ssh/id_ts_rsa
    ServerAliveInterval 30

Host bitbucket.org-TSomKes
    User git
    IdentityFile ~/.ssh/id_ts_rsa

    # Making this work at the library
    HostName altssh.bitbucket.org
    Port 443

